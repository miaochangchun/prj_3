<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.miao.po">
    <class name="Subject" table="tb_subject">
        <id name="subjectID" column="subjectID">
            <generator class="native"></generator>
        </id>

        <property name="subjectTitle" column="subjectTitle" />
        <property name="subjectOptionA" column="subjectOptionA" />
        <property name="subjectOptionB" column="subjectOptionB" />
        <property name="subjectOptionC" column="subjectOptionC" />
        <property name="subjectOptionD" column="subjectOptionD" />
        <property name="subjectAnswer" column="subjectAnswer" />
        <property name="subjectParse" column="subjectParse" />
    </class>
</hibernate-mapping>